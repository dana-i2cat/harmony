<?xml version="1.0" encoding="UTF-8"?><config defaultPDP="pdp" defaultAttributeFactory="attr"          defaultCombiningAlgFactory="comb" defaultFunctionFactory="func"          xmlns="http://sunxacml.sourceforge.net/schema/config-0.3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://sunxacml.sourceforge.net/schema/config-0.3 ../schemas/xacml-config-0.3.xsd">    <pdp name="pdp">      <attributeFinderModule class="com.sun.xacml.finder.impl.CurrentEnvModule"/>      <attributeFinderModule class="com.sun.xacml.finder.impl.SelectorModule"/>      <policyFinderModule class="com.sun.xacml.finder.impl.FilePolicyModule">        <list>          <string>IIA007Policy.xml</string>          <string>IIA008Policy.xml</string>        </list>      </policyFinderModule>    </pdp>    <attributeFactory name="attr" useStandardDatatypes="true"/>    <combiningAlgFactory name="comb" useStandardAlgorithms="true"/>    <functionFactory name="func" useStandardFunctions="true">      <condition>        <function class="TimeInRangeFunction"/>      </condition>    </functionFactory>  </config>