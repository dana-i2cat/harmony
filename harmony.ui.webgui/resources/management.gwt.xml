<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>

	<!-- Inherit XML support -->
	<inherits name="com.google.gwt.xml.XML"/>

	<!-- Specify the app entry point class.                   -->
	<entry-point class='client.ManagementGui'/>
	
	<!-- specify the style-sheet information -->
	<stylesheet src='ManagementGui.css' />
	
	<servlet path='/gui/server/argon' class='server.argon.ArgonManagementServiceImpl' />
  	<servlet path='/gui/server/common' class='server.common.CommonServiceImpl' />
  	<servlet path='/gui/server/nsp' class='server.nsp.NspClientProxyImpl' />
 </module>
