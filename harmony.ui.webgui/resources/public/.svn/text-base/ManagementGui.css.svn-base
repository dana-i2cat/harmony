body, html {
  height: 100%;
}

body {
  background: #f0f0f0;
  color: black;
  font-family: Arial, sans-serif;
  font-size: small;
  margin: 0px;
  padding: 0px;
}

a { 
  color: #000;
}

a:visited {
  color: #000; 
}

.gwt-DialogBox {
  -webkit-box-shadow: 0px 0px 40px #000;
  
  -moz-border-radius: 7px 7px 7px 7px;
  -webkit-border-radius: 7px;
}

.gwt-DialogBox .Caption {
  color: #fff;

  font-weight: bold;
  cursor: default;
  padding: 2px;
  
  text-align: center;
  
  -moz-border-radius: 7px 7px 0px 0px;
  -webkit-border-top-left-radius: 7px;
  -webkit-border-top-right-radius: 7px;
  
  background-image:url("op50.png");
}

.gwt-DialogBox td {
  text-align: left;
  color: #fff;
}

.gwt-DialogBox .gwt-Button {
  margin: 10px;
  border: 1px solid #A5A5A5;
}

.gwt-DialogBoxContent {
  width: 24em;
  font-size: 80%;
  padding: 10px;
  text-align: left;
  background-image:url("op75.png");
  
  -moz-border-radius: 0px 0px 7px 7px;
  -webkit-border-bottom-left-radius: 7px;
  -webkit-border-bottom-right-radius: 7px;
}

.gwt-LoginBoxContent {
  width: 24em;
  font-size: 80%;
  padding: 10px;
  text-align: left;
  background-image:url("op75.png");
  
  -moz-border-radius: 0px 0px 7px 7px;
  -webkit-border-bottom-left-radius: 7px;
  -webkit-border-bottom-right-radius: 7px;
}

.gwt-MenuBar {
  background: #c3d9ff;
  cursor: default;
}

.gwt-MenuItem {
  font-size: 80%;
  margin: 2px;
  cursor: default;
}

.gwt-MenuItem-selected {
  background: #e8eef7;
}

.gwt-StackPanel {
  border: 1px solid #A5A5A5;
  background: #D4DDE6;
 
}

.gwt-StackPanel .gwt-StackPanelItem {
  cursor: pointer;
  cursor: hand;
  background: #3875D7;
  background-image:url("idle.png");
  margin-top: 10px;
  margin-bottom: 10px;
  margin-left: 5px;
  padding: 5px;
  border: 0px;
}

.gwt-StackPanel .gwt-StackPanelItem-selected {
  font-weight:bold;
  color: #FFFFFF;
  background: #3875D7;
  background-image:url("selected.png");
  margin-top: 10px;
  margin-bottom: 10px;
  margin-left: 5px;
  padding: 5px;
  border: 0px;
}



.gwt-Tree {
}

.gwt-Tree .gwt-TreeItem span * {
  margin: 0 4px;
}

.gwt-Tree .gwt-TreeItem {
  font-size: 80%;
  cursor: default;
}

.gwt-Tree .gwt-TreeItem{
  padding: 2px 0;
}

.gwt-Tree .gwt-TreeItem-selected{
  background: #3875D7;
  padding:2px;
  //font-weight: bold;
  color: #FFF;
  
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px; 
}

.gwt-Suggestion {
  background: #FFFFFF;
  cursor: pointer;
  cursor: hand;
  border: 1px solid #A5A5A5;
  text-align: left;
  color: #000;
  font-size: 10px;
  
  opacity:.85;
  filter: alpha(opacity=85); 
  -moz-opacity: 0.85;
}

.demonstrator {
  background: #D4DDE6;
   border-right: 1px solid #A5A5A5;
  border-bottom: 1px solid #A5A5A5;
}

.demonstratorControls {
  background: #D4DDE6;
  width: 600px;
}

.demonstratorLegend {
  background: #D4DDE6;
  width: 200px;
}

.rightPanel {
  border: 1px solid #A5A5A5;
  background: #FFFFFF;
  width: 100%;
  height: 100%;
  padding: 2px;
  margin: 2px;
}

.frame {
align:center;
background: #fff;
 border: 0px;
 padding: 2px;
 margin: 2px;
 width: 960px;
 height: 580px;
}

.flex-table {
 border: 1px solid #A5A5A5;
 margin:2px;
 padding:0px;
}

.treeMenu {
 width: 100%;
 padding: 2px;
}
.res-Dock {
margin:2px;
padding:2px;
}

.res-Top {
 height: 30px;
 border: 0px;
 padding: 2px;
 margin:2px;
}

.gui-DefaultPage {
	align: center;
	vertical-align: middle;
	padding: 10px;
	width: 100%;
}

.gui-DefaultPage td {
	align: center;
}

.gui-TopPanel {
  height: 105px;
  border: 1px solid #A5A5A5;
  background: #fff;
}

.gui-TopPanel-Status {
  background: #D4DDE6;
  border-top: 1px solid #A5A5A5;
}

.gui-TopPanel-Label {
  background: #D4DDE6;
  color: #A5A5A5;
  padding: 3px;
}

.gui-TopPanel-Sec {
  background: #D4DDE6;
  color: #7FC241;
  padding: 3px;
}

.gui-TopPanel-UnSec {
  background: #D4DDE6;
  color: #F04831;
  padding: 3px;
}

.gui-TopPanel-Link {
  text-decoration:underline;
  cursor: pointer;
  cursor: hand;
  text-align: right;
  padding: 3px;

  background: #D4DDE6;
}

.gui-Pathbar {
  width: 100%;
  vertical-align: middle;
  border-left: 1px solid #A5A5A5;
  border-right: 1px solid #A5A5A5;
  border-bottom: 1px solid #A5A5A5;
  background: #D4DDE6;
  cursor: default;
}

.gui-PathbarContent {
  vertical-align: middle;
  font-size: 80%;
  padding: 3px;
}

.gui-DefaultPage {
  border: 1px solid #A5A5A5;
  background: #f0f0f0;
  
  padding:10px;
  
  width: 50%;
  
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px; 
}

.gui-DefaultPage-Title {
	font-weight: bold;
	font-size: 100%;
	
	padding-left:10px;
}

.gui-DefaultPage-Ruler {
	color: #A5A5A5;
	
	padding-left:10px;
}

.gui-DefaultPage-Text {
	font-size: 80%;
	
	padding-left:10px;
}

.sign-link {
 text-decoration:underline;
 cursor: pointer;
 cursor: hand;
 border-top: 1px solid #A5A5A5;
 background: #D4DDE6;
}

.gui-TopPanel table {
  font-size: 80%;
  background: #fff;
}

.gui-TopPanel .gwt-Image {
 height:90px;
 margin-right: 10px;
 margin-top: 2px;
 background: #fff;
}

.gui-ErrorPanel {
  font-size: 80%;
  padding: 10px;
  text-align: left;
  overflow: auto;
  width: 500px;
  height: 250px;
}

.gui-Clickable {
  cursor: pointer;
  cursor: hand;
}

.gui-Spacer {
  width: 100%;
}

.mail-AboutText {
  width: 24em;
  font-size: 80%;
  padding: 10px;
  text-align: left;
}

.mail-Contacts td, .mail-Tasks td {
  padding: 4px 0 0 0;
}

.mail-Contacts table {
  font-size: 80%;
}

.mail-Tasks table {
  font-size: 80%;
}

.gwt-TabPanel {
background: #FFFFFF;
}

.gwt-TabBar {
  padding-top: 2px;
  border-bottom: 1px solid #A5A5A5;
}

.gwt-TabBar .gwt-TabBarFirst {
}

.gwt-TabBar .gwt-TabBarRest {
}

.gwt-TabBar .gwt-TabBarItem {
  cursor: pointer;
  cursor: hand;
  margin-right: 2px;
  background: #D4DDE6;
  border-left: 1px solid #A5A5A5;
  border-right: 1px solid #A5A5A5;
  border-top: 1px solid #A5A5A5;
  padding: 5px;
}

.gwt-TabBar .gwt-TabBarItem-selected {
  font-weight: bold;
  cursor: default;
  margin-right: 2px;
  background: #3875D7;
  color: #FFF;
  border-left: 1px solid #A5A5A5;
  border-right: 1px solid #A5A5A5;
  border-top: 1px solid #A5A5A5;
  padding: 5px;
}

.mail-List {
  margin:0px;
  border-left: 1px solid #A5A5A5;
  border-right: 1px solid #A5A5A5;
  border-bottom: 1px solid #A5A5A5;
  font-size: 80%;
  cursor: pointer;
  cursor: hand;
}



.gwt-DisclosurePanel-open .header {
border: none;
}

.gwt-DisclosurePanel-open .content td {
border: none;
}


.gwt-DisclosurePanel-closed .header td {
}


.mail-List  td {
#  border-top: 1px solid #fff;
#  border-bottom: 1px solid #fff;
  padding: 2px 0 2px 10px;
}


.mail-ListHeader {
  background: #D4DDE6;
  padding: 4px;
  font-weight: bold;
  font-size: 120%;
}

.mail-ListHeader .mail-ListNavBar .gwt-HTML {
  font-weight: bold;
}

.mail-ListHeader td {
  padding: 5px;
  font-weight: bold;
  border-top: 1px solid #A5A5A5;
  border-bottom: 1px solid #A5A5A5;
}


.res-SelectedRow .gwt-Disclosure-Panel-open td {
  border-bottom: 0px solid #A5A5A5;
  border-top: 0px solid #A5A5A5;
}

.res-SelectedRow .gwt-Disclosure-Panel-closed td {
  border-bottom: 0px solid #A5A5A5;
  border-top: 0px solid #A5A5A5;
}

.res-SelectedRow td {
  border-bottom: 1px solid #A5A5A5;
  border-top: 1px solid #A5A5A5;
}

.res-SelectedRow {
  background: #ECF3FE;
}

.res-service-row td {
  border-bottom: 0px solid #A5A5A5;
  border-top: 0px solid #A5A5A5;
  background: #CCFFFF;
  font-weight:bold;
}

.mail-Toolbar .gwt-Image {
}

.mail-ToolButton {
  font-size: 80%;
  width: 10em;
}

.mail-Detail {
  border: 1px solid #A5A5A5;
  margin-top: 4px;
}

.mail-DetailHeader {
  background: #eee;
  border-bottom: 1px solid #A5A5A5;
  padding: 6px 4px;
}

.mail-DetailHeader td {
  padding: 0;
}

.mail-DetailInner {
  background-color: white;
  font-size: 80%;
}

.mail-DetailSubject {
  padding: 2px 10px;
  font-weight: bold;
}

.mail-DetailSender {
  font-size: 80%;
  padding: 2px 10px;
}

.mail-DetailRecipient {
  font-size: 80%;
  padding: 2px 10px;
}

.mail-DetailBody {
  line-height: 150%;
  padding: 20px 40px 20px 10px;
  font-family: 'Times New Roman', Times, serif;
}

.mail-ContactPopup {
  background: #fff;
  border: 1px solid #A5A5A5;
  padding: 4px;
}

.mail-ContactPopupName {
  font-size: 80%;
  font-weight: bold;
}

.mail-ContactPopupEmail {
  font-size: 80%;
  font-style: italic;
}

.mail-StackContent {
  height: 100%;
  padding: 0 0.5em;
  border-left: 1px solid #A5A5A5;
  border-right: 1px solid #A5A5A5;
}

.mail-Contacts {
  border-bottom: none;
}

.list-Detail {
	color: #AAAAAA;
}

.list-Now {
	background: #61AA34;
}